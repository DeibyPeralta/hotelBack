{
    "compilerOptions": {
      "target": "es2016",    
      "module": "commonjs",                               
      "typeRoots": ["./node_modules/@types", "./src/typings"],
      "outDir": "./dist", 
      "esModuleInterop": true,                            
      "forceConsistentCasingInFileNames": true,           
      "strict": true, 
      "skipLibCheck": true  
    },
    "include": ["**/**/*.ts"],
    "exclude": ["node_modules"]
  }
  
  